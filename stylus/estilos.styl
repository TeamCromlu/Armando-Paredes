*
	margin 0
	padding 0
	font-family sans-serif
.Header-fixed
		background #00144C
		
@media screen and (min-width 768px)
	body
		width 100%
		position relative
		display flex
	.Header-fixed
		background #00144C
		width 30%
		display inline-block
		text-align top
		position fixed
		height 100vh	
	.Body_Main
		&__box
			width 70%
			display inline-block
			text-align top
			margin-left 30%
		
@import 'nib'
@import 'header.styl'